<img {{bind-attr src=heroUrl}} {{bind-attr class="heroImage"}} />
<h2 {{bind-attr class=":hero-title"}}>
  Find Event In Your City </h2>
<h3 {{bind-attr class=":hero-text"}}> 
  Check Out New Events. Map Your Scene. Explore. </h3>

<div {{bind-attr class=":hero-buttons"}} >
  {{#each button in heroButtons}}
    {{#anchor-link class=button.class id=button.attrId customHref=button.anchor}}
      {{button.content}}
    {{/anchor-link}}
  {{/each}}
</div>
